{
  "name": "kunafa-client",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Emad Shaaban",
    "email": "emad.shaaban92@gmail.com"
  },
  "main": "dist/js/kunafa.bundle.js",
  "scripts": {
    "transpile": "yarn run transpile:babel && yarn run transpile:flow",
    "transpile:babel": "rm -rf lib && babel -d lib src --ignore '**/*.test.js' ",
    "transpile:flow": "flow-copy-source src lib --ignore '**/*.test.js' ",
    "build": "webpack",
    "flow": "flow",
    "test": "jest",
    "test:watch": "jest --watch",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/flow-typed/**",
      "!**/lib/**",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/index.js"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "moment": "^2.17.1",
    "pouchdb": "^6.3.4",
    "pouchdb-find": "^6.3.4",
    "ramda": "^0.24.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "reselect-map": "^1.0.3",
    "util": "^0.10.3",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-jest": "^21.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.2.0",
    "jest": "^21.0.1",
    "redux-testkit": "^1.0.6",
    "regenerator-runtime": "^0.11.0",
    "webpack": "^3.6.0"
  }
}
